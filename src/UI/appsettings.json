{
  "HttpClientRetryCount": "3",
  "HttpClientExceptionsAllowedBeforeBreaking": "5",
  "HttpClientTimeout": 240, // seconds - set high so you can debug without timeout 
  "TelemetryNameForService": "UI",
  "AppSettings": {
    "SiteTitle": "Memory Lane",
    "CacheDbResults": true
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Trace",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}